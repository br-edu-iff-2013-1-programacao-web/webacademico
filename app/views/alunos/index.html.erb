<h1>Listing alunos</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Matricula</th>
    <th>Pai</th>
    <th>Mae</th>
    <th>Cpf</th>
    <th>Rg</th>
    <th>Endereco</th>
    <th>Bairro</th>
    <th>Cidade</th>
    <th>Uf</th>
    <th>Email</th>
    <th>Imageurl</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @alunos.each do |aluno| %>
  <tr>
    <td><%= aluno.nome %></td>
    <td><%= aluno.matricula %></td>
    <td><%= aluno.pai %></td>
    <td><%= aluno.mae %></td>
    <td><%= aluno.cpf %></td>
    <td><%= aluno.rg %></td>
    <td><%= aluno.endereco %></td>
    <td><%= aluno.bairro %></td>
    <td><%= aluno.cidade %></td>
    <td><%= aluno.uf %></td>
    <td><%= aluno.email %></td>
    <td><%= aluno.imageurl %></td>
    <td><%= link_to 'Show', aluno %></td>
    <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
    <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Aluno', new_aluno_path %>
