<%= form_for(@diario) do |f| %>

    <h2><%= I18n.t "activerecord.models.diario" %></h2>

	<p><%= f.label :ano %><br /> 
	<%= f.text_field :ano %></p>
	
	<p><%= f.label :semestre %><br />
	<%= f.text_field :semestre %></p>
	
    <p><%= f.label :turno %><br />
	<%= f.text_field :turno %></p>
	
	<p><%= f.label :pasta %><br />
	<%= f.text_field :pasta %></p>
	
	<p><%= f.label :grade_id %><br />
	<%= f.collection_select :grade_id, Grade.all, :id, :referencia %> </p>

	<p><%= f.label :docente_id %><br />
	<%= f.collection_select :docente_id, Docente.all, :id, :nome %> </p>

	<div><%= f.submit %></div>

<% end %>
