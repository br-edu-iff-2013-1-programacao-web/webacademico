<%= form_for(@matriculado) do |f| %>

    <h2><%= I18n.t "activerecord.models.matriculado" %></h2>

	<p><%= f.label :periodo %><br /> 
	<%= f.text_field :periodo %></p>
	
	<p><%= f.label :estadomatricula %><br />
	<%= f.text_field :estadomatricula %></p>
	
	<p><%= f.label :aluno_id %><br />
	<%= f.collection_select :aluno_id, Aluno.all, :id, :nome %> </p>

	<p><%= f.label :curso_id %><br />
	<%= f.collection_select :curso_id, Curso.all, :id, :nome %> </p>

	<div><%= f.submit %></div>

<% end %>
