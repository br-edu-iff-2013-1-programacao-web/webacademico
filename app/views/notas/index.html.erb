<h1>Listing notas</h1>

<table>
  <tr>
    <th>Nota</th>
    <th>Avaliacao</th>
    <th>Diarioaluno</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notas.each do |nota| %>
  <tr>
    <td><%= nota.nota %></td>
    <td><%= nota.avaliacao %></td>
    <td><%= nota.diarioaluno %></td>
    <td><%= link_to 'Show', nota %></td>
    <td><%= link_to 'Edit', edit_nota_path(nota) %></td>
    <td><%= link_to 'Destroy', nota, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Nota', new_nota_path %>
